{
  "__ref_dfx": "dfx.json reference: https://internetcomputer.org/docs/current/references/dfx-json-reference/",
  "canisters": {
    "internet_identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz",
      "remote": {
        "candid": "internet_identity.did",
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "declarations": {
        "output": "canisters/wallet-ui/src/generated/internet-identity",
        "node_compatibility": true
      }
    },
    "control_panel": {
      "type": "rust",
      "package": "control_panel",
      "candid": "canisters/control-panel/spec.did",
      "declarations": {
        "node_compatibility": true
      },
      "optimize": "cycles"
    },
    "wallet_ui": {
      "type": "assets",
      "source": ["canisters/wallet-ui/dist/"],
      "frontend": {
        "entrypoint": "canisters/wallet-ui/dist/index.html"
      },
      "build": "pnpm --filter '@dfinity/orbit-wallet-ui' build",
      "declarations": {
        "output": "canisters/wallet-ui/src/generated/wallet-ui",
        "node_compatibility": true
      }
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "dfx": "0.14.4",
  "version": 1
}
